    "view",
    "views"
  ],
  "verb": {
    "related": {
      "list": [
        "assemble",
        "templates",
        "verb"
      ]
    },
    "reflinks": [
      "matched"
    ],
    "plugins": [
      "gulp-format-md"
    ],
    "layout": "default"
